    </dependency>
    <dependency>
      <groupId>de.adito.designer.netbeans</groupId>
      <artifactId>AditoNetBeansUtil</artifactId>
      <version>${designer.version}</version>
      <exclusions>
        <exclusion>
          <groupId>de.adito.designer.netbeans</groupId>
