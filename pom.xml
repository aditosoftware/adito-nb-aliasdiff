      </exclusions>
    </dependency>
    <dependency>
      <groupId>de.adito.designer.netbeans</groupId>
      <artifactId>DesignerCommonClasses</artifactId>
      <version>${designer.version}</version>
      <exclusions>
        <exclusion>
          <groupId>de.adito.designer.netbeans</groupId>
