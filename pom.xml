      <artifactId>DesignerProperties</artifactId>
      <version>${designer.version}</version>
      <exclusions>
        <exclusion>
          <groupId>org.eclipse.jdt.core.compiler</groupId>
          <artifactId>ecj</artifactId>
        </exclusion>
        <exclusion>
          <groupId>de.adito.designer.netbeans</groupId>
