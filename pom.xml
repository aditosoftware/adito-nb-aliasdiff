      <version>${designer.version}</version>
      <exclusions>
        <exclusion>
          <groupId>de.adito.designer.netbeans</groupId>
          <artifactId>Diff</artifactId>
        </exclusion>
        <exclusion>
          <groupId>de.adito.designer.netbeans</groupId>
          <artifactId>lwaditonetbeansutilites</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>de.adito.designer.netbeans</groupId>
      <artifactId>DesignerDataObjects</artifactId>
      <version>${designer.version}</version>
      <exclusions>
        <exclusion>
          <groupId>de.adito.designer.netbeans</groupId>
