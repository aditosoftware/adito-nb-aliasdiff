      <version>${designer.version}</version>
      <exclusions>
        <exclusion>
          <groupId>de.adito.designer.netbeans</groupId>
          <artifactId>lwaditonetbeansutilites</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>de.adito.designer.netbeans</groupId>
